{
    "name": "@x-9lab/xlab",
    "version": "1.5.1",
    "description": "9lab 服务端模块",
    "versionDesc": "静态文件服务支持跨域",
    "scripts": {
        "dev": "swc src -D ./src/bin --config-file .swcrc -d dist -w",
        "compile": "swc src -D ./src/bin --config-file .swcrc -d dist",
        "build": "yarn compile && yarn gen-declaration",
        "gen-declaration": "tsc --emitDeclarationOnly",
        "changelog": "changelog"
    },
    "engines": {
        "node": ">10.0.0"
    },
    "main": "./dist/server.js",
    "types": "./@types/global.d.ts",
    "files": [
        "dist",
        "@types"
    ],
    "dependencies": {
        "koa": "2.13.4",
        "cron": "1.8.2",
        "log4js": "^1.1.1",
        "liftoff": "4.0.0",
        "minimist": "1.2.6",
        "koa-static": "5.0.0",
        "koa-router": "11.0.1",
        "koa-compress": "5.1.0",
        "@x-drive/utils": "1.1.24"
    },
    "devDependencies": {
        "tslib": "2.3.0",
        "colors": "1.4.0",
        "should": "11.2.1",
        "nodemon": "2.0.12",
        "chokidar": "3.5.3",
        "@swc/cli": "0.1.57",
        "typescript": "4.3.4",
        "@swc/core": "1.2.212",
        "@types/log4js": "2.3.5",
        "@types/node": "16.11.40",
        "@types/koa-static": "4.0.2",
        "@types/koa-router": "7.4.4",
        "@x-drive/changelog": "1.1.6"
    },
    "bin": {
        "xlab": "dist/bin/xlab"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/x-9lab/xlab.git"
    }
}